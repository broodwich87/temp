<h1>New Pass</h1>
<%= form_for :pass, url: passes_path do |f| %>
    
 
    
  <p>>
    <%= f.label :name %><br>
    <%= f.text_field(:name, {:class => 'form-control'}) %>  
  </p>
  
  <p>>
    <%= f.label :license %><br>
    <%= f.text_area(:license, {:class => 'form-control'}) %>  
  </p>
  
  <p>
    <%= f.submit({:class => 'btn btn-primary'}) %>
  </p>
<% end %>